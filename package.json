{
  "name": "japes-webapp",
  "version": "0.1.0",
  "private": false,
  "author": "Marius Modlich <marius@digitalkitchen.zone>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JunoApes/japes-webapp.git"
  },
  "bugs": {
    "url": "https://github.com/JunoApes/japes-webapp/issues"
  },
  "homepage": "https://japes.club",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:turbo": "npx turbo build",
    "serve": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo type-check",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "HUSKY=0 release-it",
    "prepare": "yarn husky install",
    "push-release": "git push --follow-tags origin main",
    "theme": "chakra-cli tokens src/lib/styles/theme/index.ts",
    "theme:watch": "chakra-cli tokens src/lib/styles/theme/index.ts --watch"
  },
  "dependencies": {
    "@chakra-ui/react": "2.4.3",
    "@cosmjs/cosmwasm-stargate": "^0.29.5",
    "@cosmjs/math": "^0.29.5",
    "@cosmjs/stargate": "^0.29.5",
    "@cosmos-kit/core": "^1.0.3",
    "@cosmos-kit/cosmostation": "^0.14.20",
    "@cosmos-kit/keplr": "^0.32.20",
    "@cosmos-kit/leap": "^0.13.20",
    "@cosmos-kit/react": "^1.0.3",
    "@cosmos-kit/vectis": "^0.14.20",
    "@cosmos-kit/walletconnect-v2": "^0.1.7",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/kanit": "^4.5.10",
    "@fontsource/sigmar-one": "^4.5.9",
    "add": "^2.0.6",
    "ahooks": "^3.7.4",
    "chain-registry": "^1.6.0",
    "framer-motion": "^8.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.7.1",
    "react-qrcode-logo": "^2.8.0",
    "react-router-dom": "^6.6.2",
    "react-simple-typewriter": "^5.0.1",
    "react-toastify": "^9.1.1",
    "release-it": "^15.6.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.3.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rollup/plugin-alias": "3.1.9",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-polyfill": "6.26.0",
    "browserify-fs": "^1.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "commitizen": "^4.2.6",
    "cosmjs-types": "^0.5.2",
    "crypto-browserify": "^3.12.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-canonical": "^37.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-canonical": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-cz": "^4.9.0",
    "https-browserify": "^1.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "node-stdlib-browser": "^1.2.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "rollup-plugin-polyfill-node": "^0.11.0",
    "semantic-release": "19.0.5",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "tty-browserify": "^0.0.1",
    "typescript": "4.8.3",
    "url": "^0.11.0",
    "vite": "^3.2.5",
    "vite-plugin-checker": "^0.5.3",
    "vite-plugin-html": "^3.2.0",
    "vite-tsconfig-paths": "^3.6.0",
    "yarn": "^1.22.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "infile": "CHANGELOG.md",
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "refactor",
              "section": "Refactor"
            },
            {
              "type": "perf",
              "section": "Performance"
            },
            {
              "type": "style",
              "section": "Visuals"
            },
            {
              "type": "docs",
              "section": "Docs"
            },
            {
              "type": "chore",
              "section": "Chore"
            },
            {
              "type": "test",
              "section": "Tests"
            }
          ]
        }
      }
    }
  }
}
